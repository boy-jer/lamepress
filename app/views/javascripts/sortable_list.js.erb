$(document).ready(function() {
<% for i in 1..10 %>
	$( "#sortable<%= i %>" ).sortable({placeholder: "ui-state-highlight", update: function() {
				//alert(AUTH_TOKEN);
				$.ajax({
				                type: 'post',
				                data: $('#sortable<%= i %>').sortable('serialize'),
				                dataType: 'script',
				                complete: function(request){
				                    $('#sortable<%= i %>').effect('highlight', {}, 500);
				                 },
				           url: $('#url<%= i %>').val()})

			}});
	$( "#sortable<%= i %>" ).disableSelection();

<% end %>
});

